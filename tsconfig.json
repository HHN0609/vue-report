{
    "exclude": [
        "node_modules"
    ],
    "include": [
        "src/**/*"
    ],
    "compilerOptions": {
        "outDir": "./lib",
        "allowJs": true,
        "jsx": "preserve",
        "sourceMap": true,
        "target": "ES6",
        "module": "ES6",
        "moduleResolution": "Node",
        "typeRoots": [
            "./typeRoot"
        ]
    }
}