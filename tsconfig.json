{
    "exclude": [
        "node_modules"
    ],
    "compilerOptions": {
        "outDir": "./lib/",
        "allowJs": true,
        "jsx": "preserve",
        "sourceMap": true,
        "target": "ES6",
        "module": "esnext",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "typeRoots": [
            "./globalDeclare"
        ]
    }
}